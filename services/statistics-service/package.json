{
    "name": "@sample/statistics-service",
    "version": "0.1.0",
    "deployment": "statistics-service",
    "description": "Statistics sample service",
    "author": "Manuel Santos <manuel.santos@mercedes-benz.com>",
    "license": "MBL",
    "private": "true",
    "repository": {
        "type": "git",
        "url": "https://github.com/nodejs-boot/node-boot.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "start": "pnpm run clean:build && pnpm run build && node dist/server.js",
        "start:prod": "pnpm run build && NODE_ENV=production node dist/server.js",
        "dev": "NODE_ENV=development nodemon",
        "nodeboot:update": "pnpm update @nodeboot/*@latest",
        "build": "tsc -p tsconfig.build.json",
        "postbuild": "npx @nodeboot/aot node-boot-aot",
        "clean:build": "rimraf ./dist",
        "lint": "eslint . --ext .js,.ts",
        "lint:fix": "pnpm lint --fix",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "test": "jest --passWithNoTests",
        "tsc": "tsc"
    },
    "dependencies": {
        "@fastify/cors": "^11.1.0",
        "@fastify/helmet": "^13.0.1",
        "@fastify/multipart": "^9.2.1",
        "@fastify/static": "^8.2.0",
        "@fastify/view": "^11.1.1",
        "@nodeboot/aot": "^1.4.0",
        "@nodeboot/authorization": "^1.3.1",
        "@nodeboot/config": "^1.3.1",
        "@nodeboot/context": "^1.13.0",
        "@nodeboot/core": "^1.15.0",
        "@nodeboot/di": "^1.4.1",
        "@nodeboot/error": "^1.3.0",
        "@nodeboot/fastify-server": "^1.8.0",
        "@nodeboot/starter-actuator": "^1.7.1",
        "@nodeboot/starter-http": "^3.5.1",
        "@nodeboot/starter-openapi": "^2.4.2",
        "@nodeboot/starter-persistence": "^1.11.0",
        "@nodeboot/starter-validation": "^1.2.1",
        "@types/luxon": "^3.0.0",
        "ajv": "^8.17.1",
        "ajv-formats": "^3.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "dd-trace": "^4.17.0",
        "fastify": "^5.6.0",
        "hash-it": "^6.0.0",
        "json-rules-engine": "^7.3.1",
        "jsonpath-plus": "^10.3.0",
        "katxupa": "^1.10.4",
        "lodash": "^4.17.21",
        "luxon": "^3.7.2",
        "mongodb": "^5.9.2",
        "reflect-metadata": "^0.2.2",
        "semver": "^7.7.1",
        "typedi": "^0.10.0",
        "typeorm": "^0.3.20",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@types/jest": "^28.1.6",
        "@types/lodash": "^4.17.17",
        "@types/node": "^20.4.2",
        "@types/semver": "^7.5.8",
        "@types/supertest": "^2.0.12"
    }
